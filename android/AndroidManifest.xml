<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.whiskeysierra"
          android:versionCode="1"
          android:versionName="1.0">
    <application android:label="@string/app_name" android:icon="@drawable/icon"
                 android:name=".gestureremote.GestureRemoteApplication"
                 android:theme="@android:style/Theme.Black.NoTitleBar">
        <activity android:name=".gestureremote.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".gestureremote.servermanagment.ServerActivity"></activity>
        <activity android:name=".gestureremote.servermanagment.NewServerActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest> 
